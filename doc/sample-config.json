{
	"session" : "task-dashboard",
	"window"  : "view",

	"auto-attach" : true,

	"layout" : {
		"v:~:p33:l2" : [
			{ "h:~:p33" : [
				"task rc.verbose:label rc.hooks:off",
				{ "v:~:p50:p33" : [
					"task rc.verbose:label rc.hooks:off summary",
					"task rc.verbose:label rc.hooks:off overdue",
					"task rc.verbose:label rc.hooks:off rc.context:none calendar"
				] }
			] },
			"task rc.verbose:label rc.hooks:off burndown.daily",
			"*!task rc.verbose:label rc.hooks:off shell"
		]
	}
}
